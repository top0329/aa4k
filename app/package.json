{
  "name": "aa4k-plugin-app",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShowcaseTV/aa4k.git"
  },
  "author": "showcase",
  "scripts": {
    "dev": "run-s msw && vite",
    "msw": "msw init ./public --no-save",
    "build": "tsc && vite build --mode development --config vite.config.ts",
    "build:prod": "tsc && vite build --mode production --config vite.config.ts",
    "build-plugin": "tsc && vite build --mode development --config vite.plugin.config.ts",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "cd ../ && husky install app/.husky",
    "langchain-update": "npx npm-check-updates langchain -u && npm i",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "init": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.mjs prod",
    "init-dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.mjs dev",
    "package": "node scripts/marge-json.mjs prod && concurrently \"npm:build\" \"npm:build-plugin\" && concurrently \"npm:package:*\" && kintone-plugin-packer --out dist/plugin.zip --ppk plugin/ppk/private.ppk plugin",
    "package:js": "cp -p dist/src/desktop/desktop.js plugin/desktop.js && cp -p dist/src/config/config.js plugin/config.js",
    "package-dev": "node scripts/marge-json.mjs dev && concurrently \"npm:build\" \"npm:build-plugin\" && concurrently \"npm:package-dev:js\" && kintone-plugin-packer --out dist/plugin-dev.zip --ppk plugin/ppk/private-dev.ppk plugin && concurrently \"npm:package-dev:upload\"",
    "package-dev:js": "cp -p dist/src/desktop/desktop.js plugin/desktop.js && cp -p dist/src/config/config.js plugin/config.js",
    "package-dev:upload": "node scripts/upload.mjs"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^5.2.0",
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-sdk/signature-v4": "^3.374.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.5.1",
    "@fortawesome/pro-regular-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "@radix-ui/themes": "^2.0.3",
    "@vanilla-extract/css": "^1.14.0",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.5",
    "html-react-parser": "^5.1.0",
    "jotai": "^2.6.1",
    "langchain": "^0.1.12",
    "react": "^18.2.0",
    "react-ace-builds": "^7.4.1",
    "react-beforeunload": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-scroll-to-bottom": "^4.2.0",
    "react-speech-recognition": "^3.10.0",
    "react-use": "^17.4.2",
    "sentence-splitter": "^5.0.0",
    "tiktoken": "^1.0.13",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@kintone/dts-gen": "^8.0.3",
    "@kintone/plugin-packer": "^8.0.4",
    "@kintone/plugin-uploader": "^9.0.3",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-mdx-gfm": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/react-vite": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@types/react": "^18.2.47",
    "@types/react-beforeunload": "^2.1.5",
    "@types/react-dom": "^18.2.18",
    "@types/react-lottie": "^1.2.10",
    "@types/react-scroll-to-bottom": "^4.2.4",
    "@types/react-speech-recognition": "^3.9.5",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vanilla-extract/vite-plugin": "^3.9.4",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "babel-plugin-macros": "^3.1.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "msw": "^2.2.0",
    "msw-storybook-addon": "beta",
    "npm-check-updates": "^16.14.12",
    "prettier": "^3.1.1",
    "rollup-plugin-replace": "^2.2.0",
    "storybook": "^8.0.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-mkcert": "^1.17.3",
    "vite-plugin-svgr": "^4.2.0"
  }
}
